<Window x:Class="NodedUI_State.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NodedUI_State"
        xmlns:lib="clr-namespace:NodedUI;assembly=NodedUI"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Canvas>
        <!-- Output node -->
        <lib:CommonModule NodeName="Test2"
                         Width="200" Height="150" Canvas.Left="400" Canvas.Top="50" >
            <!-- Additional content or styling for the output node -->
        </lib:CommonModule>

        <!-- Input node -->
        <lib:StartModule NodeName="Test1"
                         Width="200" Height="150" Canvas.Left="50" Canvas.Top="50" >
            <!-- Additional content or styling for the input node -->
        </lib:StartModule>

    </Canvas>
</Window>
